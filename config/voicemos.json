                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "create_spectrogram",
                    "signal_col": "perturb_three",
                    "sample_rate": 16000
                }
            },
            "NSIM Three": {
                "type": "NSIMNode",
                "children": [],
                "output_key": "nsim three",
                "ref_spect_key": "original_spect",
                "test_spect_key": "perturb_three"
            },
            "Perturb Four": {
                "type": "TransformNode",
                "children": ["Spectrogram Four"],
                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "_add_noise",
                    "signal_col": "vad_signal",
                    "snr": -10,
                    "output_col": "perturb_four"
                }
            },
            "Spectrogram Four": {
                "type": "TransformNode",
                "children": ["NSIM Four"],
                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "create_spectrogram",
                    "signal_col": "perturb_four",
                    "sample_rate": 16000
                }
            },
            "NSIM Four": {
                "type": "NSIMNode",
                "children": [],
                "output_key": "nsim four",
                "ref_spect_key": "original_spect",
                "test_spect_key": "perturb_four"
            },
            "Perturb Five": {
                "type": "TransformNode",
                "children": ["Spectrogram Five"],
                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "_add_noise",
                    "signal_col": "vad_signal",
                    "snr": -20,
                    "output_col": "perturb_five"
                }
            },
            "Spectrogram Five": {
                "type": "TransformNode",
                "children": ["NSIM Five"],
                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "create_spectrogram",
                    "signal_col": "perturb_five",
                    "sample_rate": 16000
                }
            },
            "NSIM Five": {
                "type": "NSIMNode",
                "children": [],
                "output_key": "nsim five",
                "ref_spect_key": "original_spect",
                "test_spect_key": "perturb_five"
            }
		}
	},
	"NSIM": {
		"type": "TransformNode",
		"children": ["Output"],
		"transform_name": "collect_columns_to_matrix",
		"target_key": "data",
		"function_args": {
    		"target_column_names": ["nsim_one", "nsim_two", "nsim_three", "nsim_four", "nsim_five"],
			"output_col": "NSIM"
		}
	},
	"Output": {
		"type": "IdentityNode"
	}

}