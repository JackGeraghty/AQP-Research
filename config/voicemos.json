{
	"Load Data": {
		"type": "LoadCSVAsDFNode",
		"children": ["Load Signals"],
		"output_key": "data",
		"path_to_csv": "resources/quickstart_tcdvoip.csv"
	},
	"Load Signals": {
		"type": "TransformNode",
		"transform_name": "apply_function",
		"children": ["Perform VAD"],
		"target_key": "data",
		"function_args": {
			"signal_col": "Ref_Wave",
			"output_col": "signal",
			"function_name": "load_signal",
			"mono": "False"
		}
	},
	"Perform VAD": {
		"type": "TransformNode",
		"children": ["Spectrogram"],
		"transform_name": "apply_function",
		"target_key": "data",
		"function_args": {
			"function_name": "VAD",
			"signal_col": "signal",
			"output_col": "vad_signal",
			"sample_rate": 16000
		}
	},
	"Spectrogram": {
		"type": "TransformNode",
		"children": ["Perturb"],
		"transform_name": "apply_function",
		"target_key": "data",
		"function_args": {
			"function_name": "create_spectrogram",
			"signal_col": "vad_signal",
			"sample_rate": 16000,
			"output_col": "original_spect"
		}
	},
	"Perturb": {
		"type": "MultiNode",
		"children": ["NSIM"],
		"root_nodes": ["Perturb One" , "Perturb Two", "Perturb Three", "Perturb Four", "Perturb Five"],
		"node_data": {
			"Perturb One": {
				"type": "TransformNode",
				"children": ["Spectrogram One"],
				"transform_name": "apply_function",
				"target_key": "data",
				"function_args": {
					"signal_col": "vad_signal",
					"function_name": "_add_noise",
					"output_col": "perturb_one",
					"snr": 10
				}
			},
			"Spectrogram One": {
				"type": "TransformNode",
				"children": ["NSIM One"],
				"transform_name": "apply_function",
				"target_key": "data",
				"function_args": {
					"function_name": "create_spectrogram",
					"signal_col": "perturb_one",
					"sample_rate": 16000
				}
			},
			"NSIM One": {
				"type": "NSIMNode",
				"children": [],
				"output_key": "nsim one",
				"ref_spect_key": "original_spect",
				"test_spect_key": "perturb_one"
			},
			"Perturb Two": {
				"type": "TransformNode",
				"children": ["Spectrogram Two"],
				"transform_name": "apply_function",
				"target_key": "data",
				"function_args": {
					"signal_col": "vad_signal",
					"function_name": "_add_noise",
					"output_col": "perturb_two",
					"snr": 20
				}
			},
			"Spectrogram Two": {
				"type": "TransformNode",
				"children": ["NSIM Two"],
				"transform_name": "apply_function",
				"target_key": "data",
				"function_args": {
					"function_name": "create_spectrogram",
					"signal_col": "perturb_two",
					"sample_rate": 16000
				}
			},
			"NSIM Two": {
				"type": "NSIMNode",
				"children": [],
				"output_key": "nsim two",
				"ref_spect_key": "original_spect",
				"test_spect_key": "perturb_two"
			},
            "Perturb Three": {
                "type": "TransformNode",
                "children": ["Spectrogram Three"],
                "transform_name": "apply_function",
                "target_key": "data",
                "function_args": {
                    "function_name": "_add_noise",
                    "signal_col": "vad_signal",
                    "snr": 30,
                    "output_col": "perturb_three"
                }
            },
            "Spectrogram Three": {
                "type": "TransformNode",
